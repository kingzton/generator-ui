{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "noEmit": true,
    "types": [
      "vite/client",
      "element-plus/global",
      "unplugin-vue-define-options/macros-global",
      "@monaco-editor/loader"
    ],
    "typeRoots": ["./node_modules/@types/", "./types"]
  },
  "include": [
    "src/**/*",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
    "node_modules/.pnpm/registry.npmmirror.com+unplugin-vue-define-options@1.3.3_vue@3.2.47/node_modules/unplugin-vue-define-options/macros-global.d.ts"
  ],
  "references": [{ "path": "./tsconfig.node.json" }]
}
